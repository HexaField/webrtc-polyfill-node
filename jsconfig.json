{
	"compilerOptions": {
		"allowJs": true,
    "baseUrl": "./",
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
    "target": "ESNext",
    "moduleResolution": "node",
    "module": "ESNext",
    "allowSyntheticDefaultImports": true
	}
}
